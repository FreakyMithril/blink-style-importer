{
  "name": "Blink Style Importer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome browser extension for import css styles on any page",
  "homepage_url": "https://chrome.google.com",
  "icons": {
    "16": "img/icons/icon16.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "browser_action.html"
  },
  "permissions": [
    "contentSettings"
  ],
  "omnibox": {
    "keyword": "extension"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/main.css"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor/jquery.js",
        "js/main.js"
      ]
    }
  ]
}