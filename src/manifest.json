{
  "name": "Blink Style Importer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chrome browser extension for import css styles on any page",
  "icons": {
    "16": "img/icons/icon16.png",
    "48": "img/icons/icon48.png",
    "128": "img/icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icons/icon19.png",
    "default_title": "Import styles on page",
    "default_popup": "index.html"
  },
  "permissions": [
    "contentSettings",
    "tabs",
    "bookmarks",
    "contextMenus",
    "notifications",
    "storage",
    "activeTab",
    "http://userstyles.org/",
    "unlimitedStorage",
    "http://stylebot.me/",
    "<all_urls>"
  ],
  "omnibox": {
    "keyword": "extension"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "css/main.css"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor/jquery.js",
        "js/main.js"
      ]
    }
  ]
}